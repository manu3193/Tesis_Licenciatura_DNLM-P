keywords = {Index Terms— Nonlocal mean filtering,REVISARopt,SIMD,VIP,electron cryomicroscopy,image denoising,image vectorization,optimizacion,paralelo,parallel image processing},
mendeley-groups = {NLM Parallelizations},
