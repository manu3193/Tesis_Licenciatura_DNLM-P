@article{Orchard2008,
abstract = {Nonlocal-means (NL-means) is an image denoising method that replaces each pixel by a weighted average of all the pixels in the image. Unfortunately, the method requires the computation of the weighting terms for all possible pairs of pixels, making it computationally expensive. Some short-cuts assign a weight of zero to any pixel pairs whose neighbourhood averages are too dissimilar. In this paper, we propose an alternative strategy that uses the SVD to more efficiently eliminate pixel pairs that are dissimilar. Experiments comparing this method against other NL-means speed-up strategies show that its refined discrimination between similar and dissimilar pixel neighbourhoods significantly improves the denoising effect.},
author = {Orchard, Jeff and Ebrahimi, Mehran and Wong, Alexander},
doi = {10.1109/ICIP.2008.4712109},
file = {:home/mzumbado/Downloads/Efficient20Nonlocal-Means20Denoising20using20the20SVD.pdf:pdf},
isbn = {1424417643},
issn = {15224880},
journal = {Proceedings - International Conference on Image Processing, ICIP},
keywords = {Denoising,Nonlocal-means,SVD},
mendeley-groups = {NLM Parallelizations},
number = {January},
pages = {1732--1735},
title = {{Efficient nonlocal-means denoising using the SVD}},
year = {2008}
